{
  "name": "ar-soccerbooth",
  "version": "1.1.0",
  "description": "Portrait 9:16 AR photo booth kiosk with Google Cloud integration",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "tsc && vite build && npm run build:electron",
    "build:electron": "tsc -p tsconfig.electron.json",
    "preview": "vite preview",
    "package": "npm run build && electron-builder",
    "package:win": "npm run build && electron-builder --win"
  },
  "keywords": [
    "electron",
    "mediapipe",
    "photo-booth",
    "ar",
    "react-three-fiber",
    "kiosk"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/qrcode": "^1.5.5",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@mediapipe/tasks-vision": "^0.10.8",
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^8.15.0",
    "firebase": "^10.7.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.159.0",
    "zustand": "^4.4.7"
  },
  "build": {
    "appId": "com.ar-soccerbooth.app",
    "productName": "AR Soccer Booth",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "allowToChangeInstallationDirectory": false
    }
  }
}
